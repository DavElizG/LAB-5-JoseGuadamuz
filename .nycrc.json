{
  "all": true,
  "include": [
    "server.js",
    "libs/**/*.js"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "coverage/**",
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary",
    "lcov",
    "json-summary"
  ],
  "report-dir": "./coverage",
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "watermarks": {
    "lines": [80, 95],
    "functions": [80, 95],
    "branches": [80, 95],
    "statements": [80, 95]
  }
}
